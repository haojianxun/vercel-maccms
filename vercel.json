{
  "version": 2,
  "builds": [
    {
      "use": "@vercel/go",
      "src": "api/client.go",
      "config": {
        "zeroConfig": true
      }
    },
    {
      "use": "@vercel/static",
      "src": "statics/**",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/client"
    }
  ],
  "crons": [{
    "path": "/api/cron",
    "schedule": "* * * * *"
  }]
}