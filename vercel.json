{
  "version": 2,
  "builds": [
    {
      "use": "@vercel/go",
      "src": "api/client.go",
      "config": {
        "zeroConfig": true
      }
    },
    {
      "use": "@vercel/static",
      "src": "templates/statics/**",
      "config": {
        "basePath": "../../"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/client"
    }
  ]
}